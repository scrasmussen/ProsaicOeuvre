# -*-sh-*-
#!/bin/bash

echo ""
echo "List containers"
echo "$ docker ps -a"
echo ""
echo "List images"
echo "$ docker images"
echo ""
echo "Remove containers"
echo "$ docker rm [id]"
echo ""
echo "Remove images"
echo "$ docker rmi [id]"
echo ""
echo "Print"
echo "$ docker container prune; docker image prune; docker volume prune"
echo ""
echo "Checkout PR, push back to upstream branch"
echo "$ git push https://github.com/user/repo-name.git username-branchname:branchname"
echo ""
echo "After changing .gitmodules sync everything"
echo "$ git submodule sync"
